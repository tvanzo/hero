<div class="col-lg-6 center p-t-30">
<div id="formBox2">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  
  
  
  <span class="login100-form-title p-b-34">
						Account Login
					</span>
	<div class="wrap-input100">
    
    <%= f.text_field :first_name, autofocus: true, :id => 'first_name', :class=>"input100", placeholder: "First Name"%>
    <%= f.text_field :last_name, autofocus: true, :id => 'last_name', :class=>"input100", placeholder: "Last Name"%>
  </div>				
  
  <div class="wrap-input100">
    
    <%= f.email_field :email, autofocus: true,  :class=>"input100",autocomplete: "email", placeholder: "Email"%>
  </div>	
  
<div class="wrap-input100">
  
    <%= f.password_field :password, autocomplete: "off", :class=>"input100",placeholder: "Password " %>
</div>

  <div class="wrap-input100">
    
    <%= f.password_field :password_confirmation, autocomplete: "off",:class=>"input100",placeholder: "Confirm Password " %>
  </div>
  
  <div class="container-login100-form-btn">
  <%= f.submit 'Sign in', :class => 'login100-form-btn' %>
  </div>

<% end %>

<%= render "devise/shared/links" %>

</div>
</div>